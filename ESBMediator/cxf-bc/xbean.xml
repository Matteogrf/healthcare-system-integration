<?xml version="1.0" encoding="UTF-8"?>
<beans
     xmlns:cxfbc="http://servicemix.apache.org/cxfbc/1.0"
     xmlns="http://xbean.org/schemas/spring/1.0"
     xmlns:wsb="http://events.laboratory.unitn.it"
     xmlns:myws="http://aibmp/ImageSearch/"
     xmlns:tns="http://it.unitn/">

     <classpath>
          <location>.</location>
     </classpath>

          
     <cxfbc:consumer
     	wsdl="classpath:Events.wsdl"
        targetEndpoint="staticRouting"
        targetService="tns:processEventRequest"
        useJBIWrapper="false"
        useSOAPEnvelope="false"
        >     
     </cxfbc:consumer>
     
          <cxfbc:provider
          wsdl="classpath:DwhWS.wsdl"
          service="wsb:DwhWS"
          endpoint="DwhWS"
          interfaceName="DwhWSSOAP"
          useJBIWrapper="false"
          useSOAPEnvelope="false" />

          
          
    
          
     

</beans>

